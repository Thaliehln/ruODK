        </item>
      </select1>
      <upload ref="/data/habitat/morphological_type_photo" mediatype="image/*">
        <label ref="jr:itext('/data/habitat/morphological_type_photo:label')"/>
        <hint ref="jr:itext('/data/habitat/morphological_type_photo:hint')"/>
      </upload>
    </group>
