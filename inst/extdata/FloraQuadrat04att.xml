      </input>
      <upload ref="/data/location/quadrat_photo" mediatype="image/*">
        <label ref="jr:itext('/data/location/quadrat_photo:label')"/>
        <hint ref="jr:itext('/data/location/quadrat_photo:hint')"/>
      </upload>
      <input ref="/data/location/corner1">
        <label ref="jr:itext('/data/location/corner1:label')"/>
